<odoo>
    <data>

        <template id="calc" name="Rootfs calculator">
            <t t-call="website.layout">
                <t t-set="additional_title">Calculator</t>
                <t t-set="pageName" t-value="'rootfsCalc'"/>
                <div id="wrap">
                    <section class="container" style="font-size: 16px !important;">
                        <form accept-charset="UTF-8" action="/website_form/" class="s_website_form mt32 row center-block" data-model_name="rootfscalc.rootfscalc" data-force_action="" data-success_page="/calc" enctype="multipart/form-data" method="post">
                            <div class="o_website_form_fields">
                                <div class="col-xs-12 col-sm-8">
                                    <div class="form-group form-field form-field-integer customfontsize custommargin" data-model-field="true" data-optional="true">
                                        <label class="control-label pull-left" for="workstations">Workstations</label>
                                        <input class="form-control o_website_form_input pull-right" name="workstations" style="width:15em;" type="number"/>
                                        <div class="clearfix"/></div>
                                    <div class="form-group form-field  form-field-integer customfontsize custommargin" data-model-field="true" data-optional="true">
                                        <label class="control-label pull-left" for="printers">Printers</label>
                                        <input class="form-control o_website_form_input pull-right" name="printers" style="width: 15em;" type="number"/>
                                        <div class="clearfix"/></div>
                                    <div class="row hidden-xs">
                                        <div class="col-xs-12 text-center">
                                            <h2 class="o_default_snippet_text" style="margin-top:0;">Security</h2>
                                        </div>
                                        <div class="form-group form-field form-field-boolean customfontsize col-xs-12 col-sm-6 text-center" data-model-field="true" data-optional="true">
                                            <div>
              <span class="fa fa-shield fa-5x" data-original-title="" title="" style=""/>
                                                <span class="fa fa-suitcase fa-4x" aria-hidden="true"/>
                                            </div>
                                            <label class="control-label" for="security">
                                                <input class="o_website_form_input customcheckbox" value="basic" name="security" type="radio"/>
                                                <span style="font-size:1.4em;vertical-align:3%;font-weight:400;">Basic</span>
                                            </label>
                                            <p class="text-muted o_default_snippet_text" style="font-size:0.8em;">Here will be written some garbage describing this package of services.</p>
                                        </div>
                                        <div class="form-group form-field form-field-boolean customfontsize col-xs-12 col-sm-6 text-center" data-model-field="true" data-optional="true">
                                            <div>
              <span class="fa fa-thumbs-up fa-5x" data-original-title="" title="" style=""/>
                                                <span class="fa fa-lock fa-4x" aria-hidden="true"/>
                                            </div>
                                            <label class="control-label" for="security">
                                                <input class="o_website_form_input customcheckbox" value="advanced" name="security" type="radio"/>
                                                <span style="font-size:1.4em;vertical-align:3%;font-weight:400;">Advanced</span>
                                            </label>
                                            <p class="text-muted o_default_snippet_text" style="font-size:0.8em;">And here will be written some garbage describing this package of services.</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-4 pt-0 pl-0 checkboxblock">
                                    <div class="form-group form-field form-field-boolean customfontsize mt-0" data-model-field="true" data-optional="true" style="line-height:1.3 !important;">
                                        <label class="control-label mt-0 p-0" for="router">
                                            <input class="o_website_form_input customcheckbox mt-0" value="1" name="router" type="checkbox"/>
                                            <span class="customverticalalign"> Router1 </span>
                                        </label>
                                    </div>
                                    <div class="o_not_editable hidden o_default_snippet_text customfontsize"> This placeholder prevents its parent from disappearing when emptied </div>
                                    <div class="form-group form-field  form-field-boolean customfontsize" data-model-field="true" data-optional="true">
                                        <label class="control-label" for="ads">
                                            <input class="o_website_form_input customcheckbox" value="1" name="ads" type="checkbox"/>
                                            <span class="customverticalalign"> Ads</span>
                                        </label>
                                    </div>
                                    <div class="form-group form-field  form-field-boolean customfontsize" data-model-field="true" data-optional="true">
                                        <label class="control-label" for="fileserver">
                                            <input class="o_website_form_input customcheckbox" value="1" name="fileserver" type="checkbox"/>
                                            <span class="customverticalalign"> Fileserver</span>
                                        </label>
                                    </div>
                                    <div class="form-group form-field  form-field-boolean customfontsize" data-model-field="true" data-optional="true">
                                        <label class="control-label" for="mailserver">
                                            <input class="o_website_form_input customcheckbox" value="1" name="mailserver" type="checkbox"/>
                                            <span class="customverticalalign"> Mailserver</span>
                                        </label>
                                    </div>
                                    <div class="form-group form-field  form-field-boolean customfontsize" data-model-field="true" data-optional="true">
                                        <label class="control-label" for="oousers">
                                            <input class="o_website_form_input customcheckbox" value="1" name="oousers" type="checkbox"/>
                                            <span class="customverticalalign"> Out of office users</span>
                                        </label>
                                    </div>
                                    <div class="form-group form-field  form-field-boolean customfontsize" data-model-field="true" data-optional="true">
                                        <label class="control-label" for="ts">
                                            <input class="o_website_form_input customcheckbox" value="1" name="ts" type="checkbox"/>
                                            <span class="customverticalalign"> Ts</span>
                                        </label>
                                    </div>
                                    <div class="form-group form-field  form-field-boolean customfontsize" data-model-field="true" data-optional="true">
                                        <label class="control-label" for="backup">
                                            <input class="o_website_form_input customcheckbox" value="1" name="backup" type="checkbox"/>
                                            <span class="customverticalalign"> Backup</span>
                                        </label>
                                    </div>
                                </div>
                                <div class="clearfix"/>
                                <div class="col-xs-12 text-center hidden visible-xs">
                                    <h2 class="o_default_snippet_text" style="margin-top:0;">Security</h2>
                                </div>
                                <div class="clearfix"/>
                                <div class="form-group form-field form-field-boolean customfontsize col-xs-12 col-sm-6 text-center hidden visible-xs" data-model-field="true" data-optional="true">
                                    <div>
                                        <span class="fa fa-suitcase fa-4x" aria-hidden="true"/>
                                    </div>
                                    <label class="control-label" for="security">
                                        <input class="o_website_form_input customcheckbox" value="basic" name="security" type="radio"/>
                                        <span style="font-size:1.4em;vertical-align:3%;font-weight:400;">Basic</span>
                                    </label>
                                    <p class="text-muted o_default_snippet_text" style="font-size:0.8em;">Here will be written some garbage describing this package of services.</p>
                                </div>
                                <div class="form-group form-field form-field-boolean customfontsize col-xs-12 col-sm-6 text-center hidden visible-xs" data-model-field="true" data-optional="true">
                                    <div>
                                        <span class="fa fa-lock fa-4x" aria-hidden="true"/>
                                    </div>
                                    <label class="control-label" for="security">
                                        <input class="o_website_form_input customcheckbox" value="advanced" name="security" type="radio"/>
                                        <span style="font-size:1.4em;vertical-align:3%;font-weight:400;">Advanced</span>
                                    </label>
                                    <p class="text-muted o_default_snippet_text" style="font-size:0.8em;">And here will be written some garbage describing this package of services.</p>
                                </div>
                                <div class="clearfix"/>
                                <div class="col-xs-12 text-center">
                                    <h2 class="o_default_snippet_text mt-0">Tariff</h2>
                                </div>
                                <div class="clearfix"/>
                                <div class="col-xs-12 col-sm-4 customfontsize">
                                    <div class="panel over-h p-0 bs-bb">
                                        <div class="panel-heading p-0 over-h bor-0 bg-gray-light center-block" style="min-height:4em !important;">
                                            <div class="form-group form-field form-field-boolean text-center m-0 p-0 over-h bg-glass bor-b-0" data-model-field="true" data-optional="true" style="border:1px solid silver;border-radius:3px 3px 0 0">
                                                <label class="control-label bor-0 pl-0 pr-0 pb-0 text-center center-block" for="tariff" style="line-height:normal !important;padding-top:0.2em !important;">
                                                    <input class="o_website_form_input customcheckbox" value="basic" name="tariff" type="radio"/>
                                                    <span class="customverticalalign o_default_snippet_text" style="color:#ffffff;">Basic</span>
                                                </label>
                                            </div>
                                            <p class="o_default_snippet_text" style="color:#ffffff;"/>
                                        </div>
                                        <div class="panel-body p-0 m-0" style="border:1px solid silver;border-radius:0 0 3px 3px;">
                                            <ul class="list-group m-0 p-0">
                                                <li class="list-group-item o_default_snippet_text bor-0 bs-bb bor-rad-0" style="border-color:#6A8487 !important;">А small piece of shit</li>
                                                <li class="list-group-item o_default_snippet_text bor-b-0 bor-l-0 bor-r-0 bs-bb bor-rad-0" style="border-color:#6A8487 !important">Big piece of shit</li>
                                                <li class="list-group-item o_default_snippet_text bor-b-0 bor-l-0 bor-r-0 bs-bb bor-rad-0" style="border-color:#6A8487 !important">A very large piece of shit</li>
                                                <li class="list-group-item o_default_snippet_text bor-b-0 bor-l-0 bor-r-0 bs-bb" style="border-color:#6A8487 !important;">A huge piece of shit</li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-4 customfontsize">
                                    <div class="panel over-h p-0 bs-bb">
                                        <div class="panel-heading p-0 over-h bor-0 bg-beta center-block" style="min-height:4em !important;">
                                            <div class="form-group form-field form-field-boolean text-center m-0 p-0 over-h bg-glass bor-b-0" data-model-field="true" data-optional="true" style="border:1px solid #AA7C9E;border-radius:3px 3px 0 0">
                                                <label class="control-label bor-0 pl-0 pr-0 pb-0 text-center center-block" for="tariff" style="line-height:normal !important;padding-top:0.2em !important;">
                                                    <input class="o_website_form_input customcheckbox" value="standard" name="tariff" type="radio"/>
                                                    <span class="customverticalalign o_default_snippet_text" style="color:#ffffff;">Standard</span>
                                                </label>
                                            </div>
                                            <p class="o_default_snippet_text" style="color:#ffffff;"/>
                                        </div>
                                        <div class="panel-body p-0 m-0" style="border:1px solid #AA7C9E;border-radius:0 0 3px 3px;">
                                            <ul class="list-group m-0 p-0">
                                                <li class="list-group-item o_default_snippet_text bor-0 bs-bb bor-rad-0" style="border-color:#AA7C9E !important;">А small piece of shit</li>
                                                <li class="list-group-item o_default_snippet_text bor-b-0 bor-l-0 bor-r-0 bs-bb bor-rad-0" style="border-color:#AA7C9E !important">Big piece of shit</li>
                                                <li class="list-group-item o_default_snippet_text bor-b-0 bor-l-0 bor-r-0 bs-bb bor-rad-0" style="border-color:#AA7C9E !important">A very large piece of shit</li>
                                                <li class="list-group-item o_default_snippet_text bor-b-0 bor-l-0 bor-r-0 bs-bb" style="border-color:#AA7C9E !important;">A huge piece of shit</li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-4 customfontsize">
                                    <div class="panel over-h p-0 bs-bb">
                                        <div class="panel-heading p-0 over-h bor-0 bg-gamma center-block" style="min-height:4em !important;">
                                            <div class="form-group form-field form-field-boolean text-center m-0 p-0 over-h bg-glass bor-b-0" data-model-field="true" data-optional="true" style="border:1px solid #8FB791;border-radius:3px 3px 0 0">
                                                <label class="control-label bor-0 pl-0 pr-0 pb-0 text-center center-block" for="tariff" style="line-height:normal !important;padding-top:0.2em !important;">
                                                    <input class="o_website_form_input customcheckbox" value="vip" name="tariff" type="radio"/>
                                                    <span class="customverticalalign o_default_snippet_text" style="color:#ffffff;">VIP</span>
                                                </label>
                                            </div>
                                            <p class="o_default_snippet_text" style="color:#ffffff;"/>
                                        </div>
                                        <div class="panel-body p-0 m-0" style="border:1px solid #8FB791;border-radius:0 0 3px 3px;">
                                            <ul class="list-group m-0 p-0">
                                                <li class="list-group-item o_default_snippet_text bor-0 bs-bb bor-rad-0" style="border-color:#8FB791 !important;">А small piece of shit</li>
                                                <li class="list-group-item o_default_snippet_text bor-b-0 bor-l-0 bor-r-0 bs-bb bor-rad-0" style="border-color:#8FB791 !important">Big piece of shit</li>
                                                <li class="list-group-item o_default_snippet_text bor-b-0 bor-l-0 bor-r-0 bs-bb bor-rad-0" style="border-color:#8FB791 !important">A very large piece of shit</li>
                                                <li class="list-group-item o_default_snippet_text bor-b-0 bor-l-0 bor-r-0 bs-bb" style="border-color:#8FB791 !important;">A huge piece of shit</li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                <div class="clearfix"/></div>
                            <div class="row">
                                <div class="form-group col-xs-12 col-sm-2 mt-0 ml-0 customfontsize formsend">
                                    <button type="button" class="btn btn-primary btn-lg o_website_form_send o_default_snippet_text text-center" style="width:100% !important">Calc</button>
                                    <span id="o_website_form_result"/>
                                </div>
                            </div>
                        </form>
                    </section>
                </div>
            </t> <!-- website.layout -->
        </template>

        <template id="result">
            <t t-call="website.layout">
                <div id="wrap">
                    <section class="container" style="font-size: 16px !important;">
                        <form accept-charset="UTF-8" action="/calc/send/" class="s_website_form mt32 row center-block" data-model_name="" data-force_action="" data-success_page="" enctype="multipart/form-data" method="post">
                            <div class="o_website_form_fields">
                                <div class="col-xs-12 col-sm-8">
                                    <div class="customfontsize custommargin" data-model-field="true" data-optional="true">
                                        <p>
                                            <t t-esc="calcsummary"/>
                                        </p>
                                        <div class="clearfix"/></div>
                                </div>
                                <div class="col-xs-12 col-sm-8">
                                    <div class="form-group form-field form-field-integer customfontsize custommargin" data-model-field="true" data-optional="true">
                                        <label class="control-label pull-left" for="email">Save to Email</label>
                                        <input class="form-control o_website_form_input pull-right" name="email" type="text"/>
                                        <div class="clearfix"/></div>
                                </div>
                                <div class="clearfix"/>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12 col-sm-2 mt-0 ml-0 customfontsize formsend">
                                    <button type="button" class="btn btn-primary btn-lg o_website_form_send o_default_snippet_text text-center" style="width:100% !important">Save</button>
                                    <span id="o_website_form_result"/>
                                </div>
                                <div class="form-group col-xs-12 col-sm-2 mt-0 ml-0 customfontsize formsend">
                                    <a href="/calc/reset" class="btn btn-primary btn-lg o_default_snippet_text text-center" style="width:100% !important">Reset</a>
                                </div>
                            </div>
                        </form>
                    </section>
                </div>
            </t>
        </template>

        <template id="add" name="Add Todo Task">
            <t t-call="website.layout">
                <t t-set="additional_title">Add Todo</t>
                <div id="wrap" class="container">
                    <div class="row">
                        <section id="forms">

                            <form method="post"
                                  class="s_website_form container-fluid form-horizontal"
                                  action="/website_form/"
                                  data-model_name="rootfscalc.rootfscalc"
                                  data-success_page="/todo"
                                  enctype="multipart/form-data" >


                                <!-- Form fields will go here! -->

                                <!-- Description text field, required -->
                                <div class="form-group form-field">
                                    <div class="col-md-3 col-sm-4 text-right">
                                        <label class="control-label" for="name">Workstations*</label>
                                    </div>
                                    <div class="col-md-7 col-sm-8">
                                        <input name="workstations" type="number" required="True"
                                               class="o_website_from_input form-control" />
                                    </div>
                                </div>


                                <!-- Form fields will go here! -->


                                <!-- Submit button -->
                                <div class="form-group">
                                    <div class="col-md-offset-3 col-md-7 col-sm-offset-4 col-sm-8">
                                        <a class="o_website_form_send btn btn-primary btn-lg">
                                            Save
                                        </a>
                                        <span id="o_website_form_result"></span>
                                    </div>
                                </div>

                            </form>
                        </section>
                    </div> <!-- rows -->
                </div> <!-- container -->
            </t> <!-- website.layout -->
        </template>

        <template id="listing">
            <t t-call="website.layout">
                <ul>
                    <li t-foreach="objects" t-as="object">
                        <a t-attf-href="#{ root }/objects/#{ object.id }">
                            <t t-esc="object.display_name"/>
                        </a>
                    </li>
                </ul>
            </t>
        </template>
        <template id="object">
            <t t-call="website.layout">
                <div class="container">
                    <div class="panel panel-default">

                        <h1><t t-esc="object.display_name"/></h1>
                        <dl>
                            <t t-foreach="object._fields" t-as="field">
                                <dt><t t-esc="field"/></dt>
                                <dd><t t-esc="object[field]"/></dd>
                            </t>
                        </dl>
                    </div>
                </div>
            </t>
        </template>
    </data>
</odoo>